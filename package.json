{
  "name": "opium-ui",
  "version": "0.1.0",
  "private": true,
  "main": "dist/index.js",
  "dependencies": {
    "react-helmet": "^6.1.0",
    "react-tooltip": "^4.2.21",
    "themeor": "^0.2.1"
  },
  "devDependencies": {
    "@types/node": "^16.0.0",
    "@types/react": "^17.0.13",
    "@types/react-dom": "^17.0.8",
    "@types/react-helmet": "^6.1.1",
    "@types/react-router-dom": "^5.1.7",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "typescript": "^4.3.5"
  },
  "scripts": {
    "start": "react-scripts start",
    "build-docs": "react-scripts build",
    "parse-icons": "node ./scripts/parse-icons",
